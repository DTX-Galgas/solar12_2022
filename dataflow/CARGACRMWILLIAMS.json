{
	"name": "CARGACRMWILLIAMS",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_CRM_Williams",
						"type": "DatasetReference"
					},
					"name": "SRCCRMWILLIAMS"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "crmParqueWilliams",
						"type": "DatasetReference"
					},
					"name": "DFcrmparquewilliams"
				},
				{
					"dataset": {
						"referenceName": "DFcrmCSVWilliams",
						"type": "DatasetReference"
					},
					"name": "DFcrmcsvWilliams"
				}
			],
			"transformations": [
				{
					"name": "SELECIONARCOLUNASWILLIAMS"
				},
				{
					"name": "SPLITCRM"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as string,",
				"          primeiro_nome as string,",
				"          ultimo_nome as string,",
				"          email as string,",
				"          sexo as string,",
				"          cidade as string,",
				"          pais as string,",
				"          codigo_pais as string,",
				"          Time_zone as string,",
				"          empresa as string,",
				"          profissao as string,",
				"          etnia as string,",
				"          idioma as string,",
				"          montadora as string,",
				"          carro as string,",
				"          ano as string,",
				"          nascimento as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SRCCRMWILLIAMS",
				"SRCCRMWILLIAMS select(mapColumn(",
				"          id,",
				"          primeiro_nome,",
				"          ultimo_nome,",
				"          email,",
				"          sexo,",
				"          cidade,",
				"          pais,",
				"          codigo_pais,",
				"          Time_zone,",
				"          empresa,",
				"          profissao,",
				"          idioma,",
				"          montadora,",
				"          carro,",
				"          ano,",
				"          nascimento",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SELECIONARCOLUNASWILLIAMS",
				"SELECIONARCOLUNASWILLIAMS split(toInteger(ano)<=1970,",
				"     toInteger(ano)>1970,",
				"     disjoint: false) ~> SPLITCRM@(ate1970, maior1970)",
				"SPLITCRM@ate1970 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DFcrmparquewilliams",
				"SPLITCRM@maior1970 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> DFcrmcsvWilliams"
			]
		}
	}
}
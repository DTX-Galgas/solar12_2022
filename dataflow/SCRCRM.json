{
	"name": "SCRCRM",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ARQ_CRM_ANDRE",
						"type": "DatasetReference"
					},
					"name": "SRVCRMANDRE"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dfcrmpqandre",
						"type": "DatasetReference"
					},
					"name": "saidacrmpqandre"
				},
				{
					"dataset": {
						"referenceName": "dfcrmandre",
						"type": "DatasetReference"
					},
					"name": "saidacrmcsvandre"
				}
			],
			"transformations": [
				{
					"name": "SelecionarColunasAndre"
				},
				{
					"name": "splitcrmandre"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as string,",
				"          primeiro_nome as string,",
				"          ultimo_nome as string,",
				"          email as string,",
				"          sexo as string,",
				"          cidade as string,",
				"          pais as string,",
				"          codigo_pais as string,",
				"          Time_zone as string,",
				"          empresa as string,",
				"          profissao as string,",
				"          etnia as string,",
				"          idioma as string,",
				"          montadora as string,",
				"          carro as string,",
				"          ano as string,",
				"          nascimento as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> SRVCRMANDRE",
				"SRVCRMANDRE select(mapColumn(",
				"          id,",
				"          primeiro_nome,",
				"          ultimo_nome,",
				"          email,",
				"          sexo,",
				"          cidade,",
				"          pais,",
				"          codigo_pais,",
				"          Time_zone,",
				"          empresa,",
				"          profissao,",
				"          idioma,",
				"          montadora,",
				"          carro,",
				"          ano,",
				"          nascimento",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> SelecionarColunasAndre",
				"SelecionarColunasAndre split(toInteger(ano)<= 1970,",
				"     toInteger(ano)>1970,",
				"     disjoint: false) ~> splitcrmandre@(Ate1970, Apos1970)",
				"splitcrmandre@Ate1970 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'parquet',",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> saidacrmpqandre",
				"splitcrmandre@Apos1970 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> saidacrmcsvandre"
			]
		}
	}
}